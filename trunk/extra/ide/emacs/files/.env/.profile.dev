# .profile.dev
# by Spence Koehler
#
# for bash shells, called by ~/.profile; calls .profile.dev.projects
#

if test -z "$PROFILE_DEV_READ"; then
  if test -z "$LOAD_COUNTER"; then
    export LOAD_COUNTER=1;
  else
    LOAD_COUNTER=$(( $LOAD_COUNTER + 1 ));
  fi

  readonly PROFILE_DEV_READ=$LOAD_COUNTER;
  export PROFILE_DEV_READ;

#  echo "loading .profile.dev... $LOAD_COUNTER ..."

  if [ "`id -u`" -eq 0 ]; then
    PATH="$HOME/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/bin/X11"
  else
    PATH="$HOME/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/bin/X11:/usr/games"
  fi

  export PATH
  umask 022

  # load .vars
  test -s .env/.vars && . .env/.vars
#  test -s .env/.classpath_def && . .env/.classpath_def

fi
